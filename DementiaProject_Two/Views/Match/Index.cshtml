@{
    ViewData["Title"] = "Match";
}

<section id="one">
    <div class="inner">
        <header>
            <h2>@ViewBag.Title</h2>
        </header>

        <div class="matchContainer">
            <div class="userViewContainer">
                <h3>100% match</h3>
                <span class="image left">
                    <img id="userviewimg" src="images/mitbillede.jpg" alt="" />
                </span>
                <div class="userInfoContainer">
                    <p><b>Navn:</b> Daniel Stuhr Petersen</p>
                    <p><b>Alder</b> 21</p>
                    <p><b>Køn</b> Mand</p>
                    <p><b>Kan godt lide:</b> Alt muligt lort</p>
                    <p><b>Kan ikke lide:</b> Ingenting</p>
                </div>
            </div>
            <div class="matchbuttons">
                <div id="swipe_dislike" class="rate"></div>
                <div class="info"></div>
                <div id="swipe_like" class="rate"></div>
            </div>
            <br />
        </div>

    </div>
</section>
